@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix obj:<http://www.openrdf.org/rdf/2009/object#>.
@prefix http:<http://www.openrdf.org/rdf/2009/httpobject#>.
@prefix calli:<http://callimachusproject.org/rdf/2009/framework#>.
@prefix purl:<http://persistent.name/rdf/2010/purl#>.

<#setUp-graphs> owl:equivalentClass calli:started;
	obj:imports purl:RemoteGraph;
	obj:groovy """
		def graphs = objectConnection.getObjects(RemoteGraph).asList()
		for (def graph : graphs) {
			graph.stayFresh()
		}
	""".

<#tearDown-graphs> owl:equivalentClass calli:stopping;
	obj:imports purl:RemoteGraph;
	obj:groovy """
		def graphs = objectConnection.getObjects(RemoteGraph).asList()
		for (def graph : graphs) {
			graph.goStale()
		}
	""".

<#setUp-domains> owl:equivalentClass calli:started;
	obj:imports purl:Domain;
	obj:groovy """
		def domains = objectConnection.getObjects(Domain).asList()
		for (def domain : domains) {
			domain.startResolving()
		}
	""".

<#tearDown-domains> owl:equivalentClass calli:stopping;
	obj:imports purl:Domain;
	obj:groovy """
		def domains = objectConnection.getObjects(Domain).asList()
		for (def domain : domains) {
			domain.stopResolving()
		}
	""".
