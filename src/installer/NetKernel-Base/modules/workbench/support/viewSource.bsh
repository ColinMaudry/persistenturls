import org.ten60.netkernel.layer1.representation.IAspectNVP;
import com.ten60.netkernel.urii.aspect.*;
main()
{ sourceName = context.sourceAspect("this:param:param",IAspectNVP.class).getValue("src");
  source = context.source("ffcpl:"+sourceName, IAspectBinaryStream.class);
  response = context.createResponseFrom(source);
  response.setMimeType("text/plain");
  context.setResponse(response);
}